{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "transform-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-syntax-object-rest-spread",
    "transform-react-jsx",
    ["module-resolver", {
      "root": ["./src"],
    }],
    ["import-rename", {"^(.*)\\.jsx$": "$1"}]
  ]
}
